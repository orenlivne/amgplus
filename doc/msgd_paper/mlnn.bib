	Issn = {0079-6123},
	Keywords = {scene recognition, gist, spatial envelope, global image feature, spatial frequency, natural image},
	Pages = {23 - 36},
	Publisher = {Elsevier},
	Series = {Progress in Brain Research},
	Title = {Chapter 2 Building the gist of a scene: the role of global image features in recognition},
